    "editor.selectionBackground": "#6B6E82",
    "editorCursor.foreground": "#50bef7",
    "editorGroup.background": "#1D1C27",
    "editorGroup.border": "#1D1C27",
    "editorGroupHeader.tabsBackground": "#1D1C27",
    "editorHoverWidget.background": "#1D1C27",
    "editorHoverWidget.border": "#686B82",
    "editorIndentGuide.background": "#2B2A3B",
    "editorLineNumber.foreground": "#686B82",
    "editorWhitespace.foreground": "#2B2A3B",

    "editorSuggestWidget.background": "#1D1C27",
    "editorSuggestWidget.border": "#686B82",
    "editorSuggestWidget.selectedBackground": "#2B2A3B",

    "editorWidget.background": "#1D1C27",

    "extensionButton.prominentBackground": "#686B82",
    "extensionButton.prominentHoverBackground": "#C8D1E3",

    "focusBorder": "#C8D1E3",
    "input.background": "#1D1C27",
    "input.border": "#686B82",

    "list.activeSelectionBackground": "#C8D1E3",
    "list.activeSelectionForeground": "#1D1C27",
    "list.focusBackground": "#C8D1E3",
    "list.focusForeground": "#1D1C27",
    "list.hoverBackground": "#C8D1E3",
    "list.hoverForeground": "#1D1C27",
    "list.highlightForeground": "#686B82",
    "list.inactiveSelectionBackground": "#1D1C27",
    "list.inactiveSelectionForeground": "#C8D1E3",

    "notification.background": "#1D1C27",

    "peekView.border": "#686B82",
    "peekViewEditor.background": "#1D1C27",
    "peekViewResult.background": "#1D1C27",
    "peekViewResult.selectionBackground": "#00FFFF",
    "peekViewTitle.background": "#1D1C27",

    "pickerGroup.border": "#686B82",

    "scrollbarSlider.activeBackground": "#EDEFF2",
    "scrollbarSlider.background": "#2B2A3B",
    "scrollbarSlider.hoverBackground": "#686B82",

    "sideBar.background": "#1D1C27",
    "sideBarSectionHeader.background": "#2B2A3B",

    "statusBar.background": "#1D1C27",
    "statusBar.debuggingForeground": "#00FF00",
