    "statusBar.noFolderBackground": "#1D1C27",
    "statusBarItem.hoverBackground": "#686B82",

    "tab.activeBackground": "#1D1C27",
    "tab.activeForeground": "#FFFFFF",
    "tab.inactiveForeground": "#686B82",
    "tab.border": "#2B2A3B",
