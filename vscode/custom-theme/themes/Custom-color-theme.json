    "titleBar.activeBackground": "#1D1C27",
    "titleBar.activeForeground": "#FFFFFF",
    "titleBar.inactiveBackground": "#1D1C27",
    "titleBar.inactiveForeground": "#686B82"
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#CED8EA",
				"background": "#1D1C27",
				"bracketContentsOptions": "underline",
				"bracketContentsForeground": "#528bff",
				"bracketsOptions": "underline",
				"bracketsForeground": "#528bff",
				"tagsOptions": "underline",
				"tagsForeground": "#528bff",
				"findHighlight": "#314365",
				"findHighlightForeground": "#528bff",
				"gutter": "#1D1C27",
				"gutterForeground": "#636d8388",
				"selectionBorder": "#bbccf51b",
				"activeGuide": "#464c55",
				"stackGuide": "#464c55",
				"highlight": "#528bff80",
				"highlightForeground": "#528bff",
				"shadow": "#1D1C27",
				"shadowWidth": "1"
      }
		},
		{
			"name": "Text and source",
			"scope": [
				"text",
				"source"
			],
			"settings": {
				"foreground": "#CED8EA"
			}
		},
		{
			"name": "Text",
			"scope": "variable.parameter.function",
			"settings": {
				"foreground": "#FF0000"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#686B82",
				"fontStyle": " italic"
			}
		},
		{
			"name": "Delimiters",
			"scope": "none",
			"settings": {
				"foreground": "#00FF00"
			}
		},
		{
			"name": "Keywords",
			"scope": "keyword",
			"settings": {
        "fontStyle": "bold",
				"foreground": "#EDEFF2"
			}
		},
		{
			"name": "Variables",
			"scope": "variable",
			"settings": {
				"foreground": "#50bef7"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"meta.require",
				"support.function"
			],
			"settings": {
				"foreground": "#50bef7"
			}
		},
		{
			"name": "Classes | Types",
			"scope": [
				"support.class",
				"entity.name.class",
				"entity.name.type.class",
				"meta.class",
				"support.type"
			],
			"settings": {
        		"fontStyle": "bold",
				"foreground": "#EDEFF2"
			}
		},
		{
			"name": "Methods",
			"scope": "keyword.other.special-method",
			"settings": {
				"foreground": "#47E3DD"
			}
		},
		{
			"name": "Strings, Inherited Class",
			"scope": [
				"string",
				"constant.other.symbol",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#00FA8E"
			}
		},
		{
			"name": "Integers",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#EDEFF2"
			}
		},
		{
			"name": "Floats",
			"scope": "none",
			"settings": {
				"foreground": "#EDEFF2"
			}
		},
		{
			"name": "Boolean",
			"scope": "none",
			"settings": {
				"foreground": "#EDEFF2"
			}
		},
		{
			"name": "Constants",
			"scope": "constant",
			"settings": {
				"foreground": "#EDEFF2"
			}
		},
		{
			"name": "Tags",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#00FA8E"
			}
		},
		{
			"name": "Attributes",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#FF00FF"
			}
		},
		{
			"name": "Attribute IDs",
			"scope": [
				"entity.other.attribute-name.id",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#00FFFF"
			}
		},
		{
			"name": "Values",
			"scope": "none",
			"settings": {
				"foreground": "#00FF00"
			}
		},
		{
			"name": "Markup",
			"scope": [
				"markup.heading punctuation.definition.heading",
				"entity.name.section",

				"markup.bold",
				"punctuation.definition.bold",

				"markup.italic",
				"punctuation.definition.italic",

        		"markup.raw.inline",
			  	"markup.list",
			  	"markup.quote"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#00FF00"
			}
		},
		{
			"name": "Units",
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#EDEFF2"
			}
		},
		{
			"name": "Separator",
			"scope": "meta.separator",
			"settings": {
				"background": "#FF0000",
				"foreground": "#FF0000"
			}
		},
		{
			"name": "Inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#E5D781"
			}
		},
		{
			"name": "Deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#FF3E57"
			}
		},
		{
			"name": "Changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#00FA8E"
			}
		},
		{
			"name": "Colors",
			"scope": "constant.other.color",
			"settings": {
				"foreground": "#FF0000"
			}
		},
		{
			"name": "Escape Characters",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#00FF00"
			}
		},
		{
			"name": "Embedded",
			"scope": [
				"punctuation.section.embedded",
				"variable.interpolation"
			],
			"settings": {
				"foreground": "#50bef7"
			}
		},
		{
			"name": "Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#EDEFF2",
				"background": "#FF3E57"
			}
		},
		{
			"name": "Broken",
			"scope": "invalid.broken",
			"settings": {
				"foreground": "#EDEFF2",
				"background": "#FF3E57"
			}
		},
		{
			"name": "Deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#EDEFF2",
				"background": "#DFD06E"
			}
		},
		{
			"name": "Unimplemented",
			"scope": "invalid.unimplemented",
			"settings": {
				"foreground": "#EDEFF2",
				"background": "#3FDFD4"
			}
		}
	]
}
